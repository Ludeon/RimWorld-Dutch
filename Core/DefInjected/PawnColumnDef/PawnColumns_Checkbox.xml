<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Volg het baasje tijdens rekrutering.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Volg het baasje tijdens veldwerk (jagen of dieren temmen).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Markeer dieren om gejaagd te worden door je jagers.</Hunt.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Slacht</Slaughter.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Tem een wild dier.</Tame.headerTip>
  
</LanguageData>